{
  "name": "nnds-components",
  "version": "0.0.2-alpha.0",
  "description": "Web Components created with Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "homepage": "https://github.com/hannelevaltanen/nnds#readme",
  "keywords": [
    "design system",
    "components",
    "stencil"
  ],
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run build && build-storybook",
    "start:stencil": "stencil build --watch",
    "build:stencil": "stencil build",
    "story": "npm run build:stencil && npm-run-all --parallel start:stencil storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.12.1",
    "@stencil/react-output-target": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/web-components": "^6.4.13",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT"
}
